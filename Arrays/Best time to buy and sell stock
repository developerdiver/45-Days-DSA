#include <bits/stdc++.h> 
int maximumProfit(vector<int> &a){
    // Write your code here.
        int n= a.size();
        int mini= a[0],profit= 0;
        for(int i=1;i<n;i++){
            int cost= a[i]-mini;
            profit= max(profit,cost);
            mini= min(mini,a[i]);
        }
        return profit;
}
